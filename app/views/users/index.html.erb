
<div class="d-flex align_items-center justify-content-center">
<h1>Users:</h1></div>
<div class="d-flex align_items-center justify-content-center">
<br>



<ol class="list-group list-group-numbered">
  <% @users.each do |user| %>

    <% if @current_user != user%>
    <li class="list-group-item">
      <%= link_to user.name, user %>
    </li>
    <%end%>
  <% end %>
</ul>

<br>
<br>
<%= button_to "Create New User", new_user_path, method: :get, class: "btn btn-primary"%>
</div>