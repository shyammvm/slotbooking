<div class="d-flex align_items-center justify-content-center">
    <h1>User Home Page</h1>
</div>
<table>

  <thead>
    <tr>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Venue</th>
      <th>Booked By</th>
    </tr>
  </thead>

  <tbody>
  <% Book.all.each do |b| %>
    <tr>
      <td><%= b.try(:start_time).try(:strftime, ("%H:%M"))%></td>
      <td><%= b.try(:start_time).try(:strftime, ("%H:%M"))%></td>
      <td><%= b.venue.name %></td>
      <td><%= b.user.name %></td>
    </tr>
    <% end %>
   </tbody> 
</table>
<%= link_to "Booking Page","/books/new" %>