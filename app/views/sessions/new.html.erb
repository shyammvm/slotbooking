<h1> Sign in</h1>

<%= form_with url: sign_in_path do |form| %>
 <div class="mb3">
      <%= form.label :email %><br>
      <%= form.text_field :email, class: "form-control" %>
      
    </div><br>

    <div class="mb3">
      <%= form.label :password %><br>
      <%= form.password_field :password_digest, class: "form-control" %><br>
     
    </div><br>

    <div class="mb3">
      <%= form.submit "Sign_in", class: "btn btn-primary" %>
    </div>
<% end%>